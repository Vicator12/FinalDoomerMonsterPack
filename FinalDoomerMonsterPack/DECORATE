// BTSX Cyberdemon missile is in BTSX projectile category

actor CyberHBoundMissile : FDHellboundC4SimpleProjectile
{
	species ""
	states
	{
	Detonation:
		TNT1 A 0 A_GiveInventory ("FDHellboundC4SelfDetonate", 1, AAPTR_TARGET)
		TNT1 A 0 A_Explode ((10 * random(1,8)) + 50, 256, 0)
		TNT1 A 0 A_Explode (10, 192, 1)
		Goto super::Detonation+3
	}
}

