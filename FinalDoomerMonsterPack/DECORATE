actor ShotgunGuyBTSXMissile : FDBTSXShotgunProjectile
{
	species ""
	Damage (6*random(1,3))
	
	states
	{
	RicochetSuccess:
		TNT1 A 10 A_CustomMissile ("ShotgunGuyBTSXMissileRicochet", 0,0, 0, CMF_TRACKOWNER, 0, AAPTR_TRACER)
		Stop
	}
}

actor ShotgunGuyBTSXMissileRicochet : ShotgunGuyBTSXMissile
{
  Damage (3*random(1,3))
  -SEEKERMISSILE
  States
  {
  Death:
	TNT1 A 0
	Goto XDeath
  }
}

